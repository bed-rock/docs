        <<span class="tag-red">script</span> <span class="atribute-yellow"> type=</span>"text/javascript" <span class="atribute-yellow"> src=</span>"../br-browser-update/src/br-browser-update.js">< /<span class="tag-red">script</span>>
      </code>
    </div>
  </div>
